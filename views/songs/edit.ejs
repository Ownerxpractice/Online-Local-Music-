<!DOCTYPE html>
<html>
<head>
  <title>Edit Song</title>
  <link rel="stylesheet" href="/styles.css">
</head>
<body>
  <%- include('../partials/navbar'); %>

  <main class="container">
    <h1>Edit Song</h1>

    <form action="/songs/<%= song.id %>/edit" method="post">
      <label>Title</label>
      <input type="text" name="title" value="<%= song.title %>" required>

      <label>City</label>
      <input type="text" name="city" value="<%= song.city %>" required>

      <button type="submit">Update</button>
    </form>
  </main>
</body>
</html>
